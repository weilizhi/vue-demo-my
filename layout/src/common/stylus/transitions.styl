.shopCartList-enter
  opacity 0 !important
  transform translateY(100%) !important
.shopCartList-enter-active
  transition 1s all  linear !important
.shopCartList-enter-to
 opacity 1 !important
  transform translateY(0) !important

.shopCartList-leave
  transform translateY(0) !important
.shopCartList-leave-active
  transition 1s all  linear !important
.shopCartList-leave-to
 
  transform translateY(100%) !important

.cartControlRemove-enter
  opacity 0
  transform translate3d(48px,0,0) rotate(0deg)
.cartControlRemove-enter-active
  transition .5s all
.cartControlRemove-enter-to
  opacity 1
  transform translate3d(0,0,0) rotate(0deg)

.cartControlRemove-leave
  opacity 1
  transform translate3d(0,0,0) rotate(0deg)
.cartControlRemove-leave-active
  transition .5s all
.cartControlRemove-leave-to
  opacity 0
  transform translate3d(48px,0,0) rotate(0deg)





.mask-enter
  opacity 0
.mask-enter-active
  transition 1s opacity
.mask-enter-to
  opacity 1

.mask-leave
  opacity 1
.mask-leave-active
  transition 1s opacity
.mask-leave-to
  opacity 0